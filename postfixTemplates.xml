    </template>
    <template id="ne@userDefined" key=".ne" provider="builtin.java" language-level="1.3" topmost="false">
      <conditions />
      <template name="fakeKey" value="org.apache.commons.collections4.CollectionUtils.isNotEmpty($EXPR$)" description="" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="nb@userDefined" key=".nb" provider="builtin.java" language-level="8" topmost="false">
