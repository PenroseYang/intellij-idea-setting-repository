    </template>
    <template id="null@userDefined" key=".null" provider="builtin.java" language-level="8" topmost="true">
      <conditions />
      <template name="fakeKey" value="def checkNull(arg) {&#10;    def result = &quot;&quot;&#10;    def elements = arg.tokenize('.').collect{it.replace('(', '').replace(')', '')}&#10;    for (int i = 0; i &lt; elements.size(); i++) {&#10;        result += elements.take(i+1).join('.') + &quot; != null&quot;&#10;        if (i &lt; elements.size() - 1) {&#10;            result += &quot; &amp;&amp; &quot;&#10;        }&#10;    }&#10;    return result&#10;}" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="toMap@userDefined" key=".toMap" provider="builtin.java" language-level="8" topmost="false">
      <conditions />
      <template name="fakeKey" value="$EXPR$.stream().collect(Collectors.toMap($END$::$END$, item -&gt; item));" description="" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="nn@userDefined" key=".nn" provider="builtin.java" language-level="8" topmost="true">
      <conditions />
